package java_basics;

public class Revesearray 
{	public static void main(String[] args)
	{ int i,k,t;
		int [] arr = new int[5];
		arr[0]= 1;
		arr[1]= 2;
		arr[2]= 3;
		arr[3]= 3;
		arr[4]= 4;
		for(int i =0;i<arr.length/2;i++)
			{
				for(int j=4;j<arr.length-1;j--)
					{
						arr[i]=arr[j];
						System.out.println(arr[j]);
					}
			}
	

	}
}


