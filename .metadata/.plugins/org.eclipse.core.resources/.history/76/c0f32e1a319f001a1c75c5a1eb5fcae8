import java.util.Scanner;

public class Pattern7 {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the no of rows");
		int n = s.nextInt();
		System.out.print("* ");
		
		
		
		
		
		for(int i=1;i<n;i++)
		{
			System.out.print("* ");
		}
		System.out.println();

}
